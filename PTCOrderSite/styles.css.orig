/* Order Page
 * ---------- */

.accordion {
    width: 800px;
    margin: 0 auto;
    border-radius:3px;
}
        
.accordionHeader {
    border-radius:3px;
    border: 1px solid #2F4F4F;
    color: white;
    background: url('images/bg-header.jpg') repeat-x center 0;
    font-family: Arial, Sans-Serif;
    font-size: 12px;
    font-weight: bold;
    padding: 5px;
    margin-top: 5px;
    cursor: pointer;
        }
        
.accordionHeaderSelected {
    border-radius:3px;
    border: 1px solid #2F4F4F;
    color: white;
    background-image: url("images/bg-accordion.png");
    font-family: Arial, Sans-Serif;
    font-size: 12px;
    font-weight: bold;
    padding: 5px;
    margin-top: 5px;
    cursor: pointer;
}
        
.accordionContent {
    border-radius:3px;
    border: 1px solid #2F4F4F;
    background-color: #D3DEEF;
    border: 1px dashed #2F4F4F;
    border-top: none;
    padding: 5px;
    padding-top: 10px;
}

.tableFirstColumn {
    text-align:left;
    width:160px;
}

.tableSecondColumn {
    width:200px;
}

.tableThirdColumn {
     text-align:left;
     width:150px;
}

.tableFourthColumn {
    width:250px;
}

table.accordionBorder { 
    border-collapse:collapse; 
}

table.accordionBorder td, table.bottomBorder th { 
    border-bottom:1px solid black;
    padding:5px; 
}

.red {
    color:red;
}

.textbox {
    border-radius:5px;
}

.dropdown {
    border-radius:3px;
}

.submitButton {
    border-radius:3px;
}

